{
   "name": "node-ts-template",
   "version": "1.0.0",
   "description": "Basic template of a node + typescript project",
   "main": "dist/index.js",
   "scripts": {
      "build": "tsc",
      "lint": "eslint --fix src",
      "test": "SET NODE_ENV=test&& mocha --recursive build/specs",
      "test-istanbul": "SET NODE_ENV=test&& nyc mocha --recursive build/specs",
      "compile-schemas": "rm src/schemas/types/*; cd src/schemas/json && json2ts -i . -o ../types",
      "start": "ts-node src/index.ts",
      "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
   },
   "author": "Maxence CHAMBRIN",
   "license": "ISC",
   "devDependencies": {
      "@types/mocha": "^9.1.0",
      "@types/node": "^15.12.2",
      "@typescript-eslint/eslint-plugin": "^5.3.0",
      "@typescript-eslint/parser": "^5.3.0",
      "chai": "^4.3.6",
      "eslint": "^8.2.0",
      "json-schema-to-typescript": "^10.1.5",
      "mocha": "^9.2.1",
      "nyc": "^15.1.0",
      "pino-pretty": "^7.2.0",
      "ts-node": "3.3.0",
      "typescript": "^4.3.2"
   },
   "dependencies": {
      "@types/bcrypt": "^5.0.0",
      "@types/chai": "^4.3.0",
      "bcrypt": "^5.0.1",
      "cookie-signature": "^1.2.0",
      "fastify": "^3.24.1",
      "fastify-cookie": "^5.3.2",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.10",
      "typeorm": "0.2.41"
   },
   "_moduleAliases": {
      "CppGenerator": "./CppGenerator.node"
   }
}
